<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 用户角色权限管理 -->
<mapper namespace="io.tingfeng.mybatis.springbootmybatis.dao.UsersMapper">
    <select id="selectAll" resultType="io.tingfeng.mybatis.springbootmybatis.model.Users">
        SELECT *
        FROM users
    </select>

    <insert id="insert" parameterType="io.tingfeng.mybatis.springbootmybatis.model.Users">
        INSERT INTO users(name, salary)
        values (#{name}, #{salary})
    </insert>

    <delete id="delete" parameterType="io.tingfeng.mybatis.springbootmybatis.model.Users">
        DELETE
        FROM users
        WHERE name = #{name}
    </delete>
</mapper>